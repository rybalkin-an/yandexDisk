    "system_folders": {
      "type": "object",
      "properties": {
        "odnoklassniki": {
          "type": "string"
        },
        "google": {
          "type": "string"
        },
        "instagram": {
          "type": "string"
        },
        "vkontakte": {
          "type": "string"
        },
        "mailru": {
          "type": "string"
        },
        "downloads": {
          "type": "string"
